DNSMASQ_DHCP_TYPES (?:DHCPREQUEST|DHCPACK|DHCPNAK|DHCPINFORM)
DHCP_CLIENTHOSTNAME (?:%{NOTSPACE}|Hostname Unsuitable for Printing)
DHCP_INTERFACECHAR [^\s:]+
DNSMASQ_DHCP_REQUEST DHCPREQUEST\(%{DHCP_INTERFACECHAR}\) %{IP:ipaddr} %{COMMONMAC:macaddr}
DNSMASQ_DHCP_ACK DHCPACK\(%{DHCP_INTERFACECHAR}\) %{IP:ipaddr} %{COMMONMAC:macaddr} %{DHCP_CLIENTHOSTNAME:hostname}
DNSMASQ_DHCP_NAK DHCPNAK\(%{DHCP_INTERFACECHAR}\) %{IP:ipaddr} %{COMMONMAC:macaddr}
DNSMASQ_DHCP_INFORM DHCPINFORM\(%{DHCP_INTERFACECHAR}\) %{IP:ipaddr} %{COMMONMAC:macaddr}

DNSMASQ_DHCP_DORA %{DNSMASQ_DHCP_REQUEST}|%{DNSMASQ_DHCP_ACK}|%{DNSMASQ_DHCP_NAK}|%{DNSMASQ_DHCP_INFORM}
DNSMASQ_DHCP_TYPE %{DNSMASQ_DHCP_TYPES:dhcp_pkt_type}
